<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link>
    <title>Document</title>
</head>
<body>
<ul>
    {% for agent in agents %}
        {% if agent.published %}
            <div class="agent">
                <a href="{{ path('agent_show', {'id' : agent.id}) }}">{{ agent.lastName }}, {{ agent.firstName }}, {{ agent.age }} ans</a>
            </div>
        {% endif %}
    {% endfor %}
</ul>
</body>
</html>